{
  "name": "@btcp/ai-agents",
  "version": "1.0.0",
  "description": "Generic, browser-compatible AI agent system with pluggable skills and action adapters",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "browser": "./src/browser.ts",
      "node": "./src/node.ts",
      "default": "./src/index.ts"
    },
    "./browser": {
      "types": "./dist/types/browser.d.ts",
      "import": "./src/browser.ts"
    },
    "./node": {
      "types": "./dist/types/node.d.ts",
      "import": "./src/node.ts"
    },
    "./agent-sdk": "./src/agent-sdk.ts",
    "./browser-agent": "./src/browser-agent.ts",
    "./sdk": "./src/core/index.ts",
    "./agents": "./src/agents/index.ts",
    "./core": "./src/core/index.ts",
    "./core/loop": "./src/core/loop/index.ts",
    "./engine": "./src/engine/index.ts",
    "./skills": "./src/skills/index.ts",
    "./tools": "./src/tools/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./commands": "./src/commands/index.ts",
    "./aliases": "./src/aliases/index.ts",
    "./resources": "./src/resources/index.ts",
    "./context": "./src/context/index.ts",
    "./planning": "./src/planning/index.ts",
    "./http": "./src/http/index.ts",
    "./types": "./src/types/index.ts",
    "./mcp": "./src/mcp/index.ts",
    "./btcp": "./src/btcp/index.ts",
    "./handlers": "./src/http/index.ts",
    "./platform": "./src/platform/index.ts",
    "./platform/browser": "./src/platform/browser/index.ts",
    "./platform/node": "./src/platform/node/index.ts",
    "./adapters": "./src/adapters/index.ts",
    "./patterns": "./src/patterns/index.ts",
    "./patterns/tools": "./src/patterns/tools/index.ts",
    "./patterns/agent": "./src/patterns/agent/index.ts",
    "./patterns/resources": "./src/patterns/resources/index.ts",
    "./patterns/hooks": "./src/patterns/hooks/index.ts",
    "./patterns/skills": "./src/patterns/skills/index.ts",
    "./patterns/subagents": "./src/patterns/subagents/index.ts",
    "./patterns/prompts": "./src/patterns/prompts/index.ts",
    "./patterns/context": "./src/patterns/context/index.ts",
    "./benchmarks": "./src/benchmarks/index.ts",
    "./tracing": "./src/tracing/index.ts"
  },
  "scripts": {
    "build": "pnpm build:prompts && pnpm build:vite",
    "build:prompts": "tsx scripts/bundle-prompts.ts",
    "build:vite": "vite build",
    "build:browser": "vite build --mode browser",
    "build:node": "vite build --mode node",
    "build:types": "tsc --project tsconfig.base.json --emitDeclarationOnly",
    "build:tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.config.unit.ts",
    "test:all": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:live": "vitest run --config vitest.config.live.ts",
    "test:live:hello": "vitest run --config vitest.config.live.ts --testNamePattern=\"Live Hello\"",
    "test:live:tool": "vitest run --config vitest.config.live.ts --testNamePattern=\"Live Tool\"",
    "test:live:complex": "vitest run --config vitest.config.live.ts --testNamePattern=\"Complex Tool\"",
    "test:watch": "vitest --config vitest.config.unit.ts",
    "test:watch:all": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "vitest run --config vitest.config.unit.ts --coverage",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "mcp:example": "tsx src/mcp/example.ts",
    "canvas-mcp:dev": "pnpm --filter=@waiboard/canvas-mcp start --http",
    "patterns:demo": "tsx src/patterns/demo.ts",
    "patterns:test": "tsx src/patterns/test-tools.ts",
    "benchmark": "tsx src/benchmarks/cli.ts",
    "benchmark:smoke": "tsx src/benchmarks/cli.ts --suite smoke",
    "benchmark:core": "tsx src/benchmarks/cli.ts --suite core",
    "benchmark:full": "tsx src/benchmarks/cli.ts --suite full",
    "benchmark:reasoning": "tsx src/benchmarks/cli.ts --suite reasoning",
    "benchmark:taod": "tsx src/benchmarks/cli.ts --suite taod",
    "benchmark:routing": "tsx src/benchmarks/cli.ts --suite routing",
    "benchmark:regression": "tsx src/benchmarks/cli.ts --suite regression",
    "benchmark:edge": "tsx src/benchmarks/cli.ts --suite edge-cases",
    "benchmark:compare": "tsx src/benchmarks/cli.ts --compare --model-group flagship",
    "benchmark:compare:fast": "tsx src/benchmarks/cli.ts --compare --model-group fast",
    "benchmark:help": "tsx src/benchmarks/cli.ts --help",
    "test:benchmark": "vitest run --config vitest.config.live.ts --testNamePattern=\"Smoke Test\"",
    "test:benchmark:full": "RUN_FULL_BENCHMARK=true vitest run --config vitest.config.live.ts"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "nanoid": "^5.0.9",
    "openai": "^4.77.0",
    "@utcp/code-mode": "^1.0.5",
    "@utcp/code-mode-mcp": "^1.0.9",
    "@utcp/mcp": "^1.0.16",
    "@utcp/sdk": "1.0.17",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mime": "^4.1.0",
    "sharp": "^0.34.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.280",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.1.4",
    "dotenv": "^16.6.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "express": "^4.21.0",
    "sharp": "^0.34.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "sharp": { "optional": true }
  }
}
